{
  "env": {
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "8"
            },
          }
        ],
        "@babel/preset-typescript",
        "minify"
      ],
      "plugins": [
        "@babel/plugin-transform-runtime"
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "8"
            }
          }
        ],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "istanbul"
      ]
    }
  }
}
